{
  "name": "payment-plus",
  "version": "2.0.0",
  "description": "Programa de facturacion y administracion de ISP",
  "main": "gulpfile.js",
  "engines": {
    "node": ">= 8.*",
    "npm": ">= 5.5.1"
  },
  "scripts": {
    "serve": "npm-run-all --parallel gulp start-webpack",
    "start-webpack": "webpack --progress --watch",
    "build": "node resources/build/build.js && gulp sass",
    "test-server": "cd application/tests && ./../../vendor/bin/phpunit && cd ./../../",
    "test:unit": "cross-env BABEL_ENV=test karma start resources/src/test/unit/karma.conf.js --single-run",
    "test:e2e": "testcafe chrome resources/test/e2e/specs",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue resources/src resources/test/unit/specs resources/test/e2e/specs",
    "get-files": "git checkout heroku && getFrom feature/enviroments src/ && getFrom feature/enviroments application/ && getFrom feature/enviroments webpack/",
    "build-and-commit": "npm run build && git commit -m 'build of($date)'",
    "heroku-push": " git push heroku heroku:master -f && heroku scale web=1 && heroku open",
    "heroku": "npm run build-and-commit && npm run heroku-push",
    "gulp": "gulp"
  },
